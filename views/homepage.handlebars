 <div>
    <h1>Welcome!</h1>
    <h5>Checkout any of our fabulous blogs below</h5>
</div>


<div class="main-container">
    {{#each blogs as |blog|}}
    <div class="card-header col-md-12 blog">
       <div>
            <h2>{{title}}</h2>
            <p>Created by:
                {{blog.user_id}}
            </p>
            <p>On:
                {{date_created}}
            </p>
         </div>
         <div id="content">
            <p>{{content}}</p>
        </div>     
    </div>
       
    {{/each}}


</div>
